{
  "name": "lego-library",
  "version": "1.0.0",
  "description": "css component library",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "expanded": "node-sass --output-style expanded --source-comments scss/legos.scss legos.css",
    "compressed": "node-sass --output-style compressed --source-comments scss/legos.scss legos.css",
    "autoprefixer": "postcss -u autoprefixer -r legos.css",
    "compile": "npm run expanded && npm run autoprefixer",
    "watch:css": "onchange 'scss/*.scss' -- npm run expanded",
    "final:css": "npm run compressed && npm run autoprefixer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morgantaylor/lego-library.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/morgantaylor/lego-library/issues"
  },
  "homepage": "https://github.com/morgantaylor/lego-library#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "node-sass": "^4.13.0",
    "onchange": "^6.1.0",
    "postcss-cli": "^6.1.3"
  },
  "dependencies": {}
}
